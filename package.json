{
  "name": "landwash-intern",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:android": "concurrently --kill-others-on-fail --names \"SERVE,CAP,LOGS\" -c \"cyan.bold,green.bold,magenta.bold\" \"npm run _dev:serve\" \"npm run _dev:cap\" \"npm run _dev:logs\"",
    "_dev:serve": "ng serve --host 0.0.0.0 --disable-host-check",
    "_dev:cap": "sleep 5 && cp capacitor.config.local.ts capacitor.config.ts && npx cap sync android && cd android && ./gradlew assembleDebug && adb uninstall com.landwash.intern || true && adb install app/build/outputs/apk/debug/app-debug.apk && cd ..",
    "_dev:logs": "adb logcat | grep '192.168.1.67:4200'"
  },
  "private": true,
  "dependencies": {
